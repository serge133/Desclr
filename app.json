{
  "expo": {
    "name": "desclr",
    "slug": "desclr",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.desclr.desclr"
    },
    "android": {
      "package": "com.desclr.desclr",
      "versionCode": 1
    },
    "description": "This is an archiving todo app",
    "githubUrl": "https://github.com/serge133/Desclr"
  }
}
